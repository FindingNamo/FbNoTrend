{
    "manifest_version": 2,
  
    "name": "FBNoTrend",
    "description": "Removes the trending news section on Facebook",
    "version": "0.2017.11.19",
  
    "content_scripts": [
        {
          "matches": ["https://www.facebook.com/*"],
          "js": ["fbNoTrend.js"]
        }
      ],
      
    "icons": { "16": "fbNoTrend16.png",
      "48": "fbNoTrend48.png",
      "128": "fbNoTrend128.png" }
}  